#include<iostream>
using namespace std;

int binary_search(int a[], int left, int right, int key) {
  int mid;
  while(left <= right) {
    mid = left + (right - left) / 2;  //for mid
    if (a[mid] == key)     // Element found in search
      return mid;
    if (a[mid] < key)         // Search in right part of list (array)
      left = mid + 1;
    else            // Search in left part of list
      right = mid - 1;
  }
  return -1;
}

int main() {
  int n;
  cin >> n;
  int a[n];
  for (int i = 0; i < n; ++i) {
    cin >> a[i];
  }
  int key;
  cin >> key;
  cout << binary_search(a, 0, n - 1, key);
  return 0;
}
